{
  "name": "react-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for react-typescript-eslint setup",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write .",
    "build": "webpack --mode production --config webpack.config.mjs",
    "dev": "webpack serve --open",
    "lint": "eslint --ext .ts,.tsx src/",
    "lint:error": "eslint --ext .ts,.tsx src/ --quiet",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/limgit/react-typescript-boilerplate.git"
  },
  "author": "HyunSu Lim",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "html-webpack-plugin": "^5.6.4",
    "prettier": "3.6.2",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
